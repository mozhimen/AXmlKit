<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="LayoutKBtnSwitch">
        <!--默认开关状态-->
        <attr name="layoutKBtnSwitch_defaultStatus" format="boolean" />
        <!--打开时颜色-->
        <attr name="layoutKBtnSwitch_bgColorOn" format="reference|color" />
        <!--关闭时颜色-->
        <attr name="layoutKBtnSwitch_bgColorOff" format="reference|color" />
        <!--中心圆颜色-->
        <attr name="layoutKBtnSwitch_btnColor" format="reference|color" />
        <!--中心圆到外边距的距离-->
        <attr name="layoutKBtnSwitch_btnMargin" format="dimension|reference" />
        <!--外边框厚度-->
        <attr name="layoutKBtnSwitch_borderWidth" format="dimension|reference" />
        <!--外边框颜色-->
        <attr name="layoutKBtnSwitch_borderColor" format="reference|color" />
        <!--变换时间-->
        <attr name="layoutKBtnSwitch_animTime" format="integer" />

    </declare-styleable>

    <declare-styleable name="LayoutKBtnSwitchApple">
        <!--默认开关状态-->
        <attr name="layoutKBtnSwitchApple_defaultStatus" format="boolean" />
        <!--打开时颜色-->
        <attr name="layoutKBtnSwitchApple_bgColorOn" format="reference|color" />
        <!--关闭时颜色-->
        <attr name="layoutKBtnSwitchApple_bgColorOff" format="reference|color" />
        <!--中心圆颜色-->
        <attr name="layoutKBtnSwitchApple_btnColor" format="reference|color" />
        <!--中心圆到外边距的距离-->
        <attr name="layoutKBtnSwitchApple_btnMargin" format="dimension|reference" />
        <!--外边框厚度-->
        <attr name="layoutKBtnSwitchApple_borderWidth" format="dimension|reference" />
        <!--外边框颜色-->
        <attr name="layoutKBtnSwitchApple_borderColor" format="reference|color" />
        <!--变换时间-->
        <attr name="layoutKBtnSwitchApple_animTime" format="integer" />

    </declare-styleable>

    <declare-styleable name="LayoutKBtnIcon">
        <attr name="layoutKBtnIcon_background" format="reference" />
        <attr name="layoutKBtnIcon_iconSrc" format="reference" />
        <attr name="layoutKBtnIcon_iconSize" format="dimension|reference" />
        <attr name="layoutKBtnIcon_iconMarginRight" format="dimension|reference" />
        <attr name="layoutKBtnIcon_text" format="string|reference" />
        <attr name="layoutKBtnIcon_textColor" format="color|reference" />
        <attr name="layoutKBtnIcon_textSize" format="dimension|reference" />
    </declare-styleable>
</resources>