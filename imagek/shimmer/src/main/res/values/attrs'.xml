<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ImageKShimmer">
        <attr name="imageKShimmer_colored" format="boolean" />
        <attr name="imageKShimmer_direction" format="enum">
            <enum name="left_to_right" value="0"/>
            <enum name="top_to_bottom" value="1"/>
            <enum name="right_to_left" value="2"/>
            <enum name="bottom_to_top" value="3"/>
        </attr>
        <attr name="imageKShimmer_shape" format="enum">
            <enum name="linear" value="0"/>
            <enum name="radial" value="1"/>
        </attr>
        <attr name="imageKShimmer_fixed_width" format="dimension"/>
        <attr name="imageKShimmer_fixed_height" format="dimension"/>
        <attr name="imageKShimmer_width_ratio" format="float"/>
        <attr name="imageKShimmer_height_ratio" format="float"/>
        <attr name="imageKShimmer_intensity" format="float"/>
        <attr name="imageKShimmer_dropoff" format="float"/>
        <attr name="imageKShimmer_tilt" format="float"/>
        <attr name="imageKShimmer_clip_to_children" format="boolean"/>
        <attr name="imageKShimmer_auto_start" format="boolean"/>
        <attr name="imageKShimmer_repeat_count" format="integer"/>
        <attr name="imageKShimmer_repeat_mode" format="enum">
            <enum name="restart" value="1"/>
            <enum name="reverse" value="2"/>
        </attr>
        <attr name="imageKShimmer_repeat_delay" format="integer"/>
        <attr name="imageKShimmer_start_delay" format="integer"/>
        <attr name="imageKShimmer_duration" format="integer"/>
        <attr name="imageKShimmer_base_color" format="color"/>
        <attr name="imageKShimmer_highlight_color" format="color"/>
        <attr name="imageKShimmer_base_alpha" format="float"/>
        <attr name="imageKShimmer_highlight_alpha" format="float"/>
    </declare-styleable>
</resources>