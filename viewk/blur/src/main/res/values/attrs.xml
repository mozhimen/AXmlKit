<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ViewKBlur">
        <attr name="viewKBlur_hasRound" format="boolean" />
        <attr name="viewKBlur_roundRadius" format="dimension" />
        <attr name="viewKBlur_barrierColor" format="color" />
        <attr name="viewKBlur_overlayColor" format="color" />
        <attr name="viewKBlur_overlayBlendMode" format="enum">
            <enum name="none" value="-1" />
            <enum name="srcOver" value="3" />
            <enum name="overlay" value="15" />
        </attr>
    </declare-styleable>

    <declare-styleable name="ViewKBlur2">
        <attr name="viewKBlur2_overlayColor" format="color"/>
    </declare-styleable>
</resources>